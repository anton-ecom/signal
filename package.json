{
  "name": "@synet/signal",
  "version": "0.1.0",
  "description": "Experimental Fractal Architecture pattern for Synet development",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synthetism/synet/packages/signal.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "dev:test": "vitest",
    "clean": "rm -rf dist",
    "coverage": "vitest run --coverage",
    "lint": "biome lint ./examples ./src  ",
    "format": "biome format --write  ./examples ./src",
    "prepublishOnly": "npm run lint && npm run build",
    "publish:dig": "npm publish --registry https://registry.dig.run",
    "unpublish:dig": "npm unpublish @synet/patterns --registry https://registry.dig.run  --force",
    "dig":" npm run unpublish:dig && npm run publish:dig"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.15.17",
    "@vitest/coverage-v8": "^3.1.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "Synet",
    "Fractal Architecture",
    "zAItgeist",
    "Synthetic Minds",
    "Fractal Patterns",
    "Pattern Recognition"
  ],
  "author": "It was already present. We tuned in and now, it speaks through code",
  "homepage": "https://synthetism.ai",
  "license": "MIT"
}
